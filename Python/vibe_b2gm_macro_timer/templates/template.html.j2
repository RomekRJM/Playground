<html>
<head>
    <meta http-equiv="content-type" content="text/html; charset=UTF-8">
    <link href="c3.css" rel="stylesheet">
    <script src="d3.v5.min.js" charset="utf-8"></script>
    <script src="c3.min.js"></script>

<body>
<h3>
    {{ replay_name }}
</h3>
<div id="chart"></div>
<script>
var chart = c3.generate({
    data: {
    x: 'x',
        columns: [
            ['x', {%- for x in time_mm_ss %}"{{ x }}",{% endfor %}],
            ['workers', {%- for w in workers %}"{{ w }}",{% endfor %}],
            ['total supply', {%- for t in total %}"{{ t }}",{% endfor %}]
        ],
    },
    axis: {
        x: {
            type: 'category',
            //tick: {
            //    format: function (x) { return parseInt(x.replace(":","") % 300 != 0); }
            //}
        }
    }
});
</script>
</body>
</html>